{
  "expo": {
    "name": "furniture-app",
    "slug": "furniture-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "build": {
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "ios": {
        "buildType": "app-bundle",
        "image": "latest"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-font"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aniketpatra96.furnitureapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.aniketpatra96.furnitureapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "env": {
      "GRADLE_OPTS": "--warning-mode=all"
    },
    "extra": {
      "eas": {
        "projectId": "1ff9b9a9-f28b-4423-b300-3fc040aa9b62"
      }
    }
  }
}
